 {
  "routes": [
    {
      "route": "/api/GetVersion",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/AuthorizeBergfestOnStrava",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/WriteArticle",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/api/GetArticle/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/GetSegmentFromStrava/*",
      "allowedRoles": [ "admin", "segmentadmin" ]
    },
    {
      "route": "/api/WriteSegment",
      "allowedRoles": [ "admin", "segmentadmin" ]
    },
    {
      "route": "/api/GetSegment/*",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/GetSegmentsWithEfforts",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/api/GetUsers",
      "allowedRoles": [ "admin" ]
    }
  ],
  "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [ "/images/*.{png,jpg,gif}", "/css/*" ]
      },
      "platform": {
        "apiRuntime": "dotnet:6.0"
      }
}