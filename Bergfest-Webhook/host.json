{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 1,
      "maxDequeueCount": 3,
      "messageEncoding": "base64"
    }
  }
}